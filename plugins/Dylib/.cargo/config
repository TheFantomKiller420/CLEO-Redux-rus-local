[build]
#target = "i686-pc-windows-msvc"
target = "x86_64-pc-windows-msvc"

[target.i686-pc-windows-msvc]
rustflags = [
    "-C", "target-feature=+crt-static",
    "-C", "link-arg=/OUT:build/dylib.cleo",
    "-L", "../SDK"
]

[target.x86_64-pc-windows-msvc]
rustflags = [
    "-C", "target-feature=+crt-static",
    "-C", "link-arg=/OUT:build/dylib64.cleo",
    "-L", "../SDK"
]
